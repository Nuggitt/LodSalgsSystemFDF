@page
@model LodSalgsSystemFDF.Pages.Indtaegter.CreateModel
@{
    ViewData["Title"] = "Create";
}
<div class="content-container">
<h1 style="margin-top:80px">
<h1>Opret Ny Indtægt</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="  ModelOnly  " class="text-danger">
            </div>

            <div class="form-group">
                <label asp-for="@Model.Indtaegt.Indtaegt_ID" class="control-label"></label>
                <input asp-for="@Model.Indtaegt.Indtaegt_ID" class="form-control" />
                <span asp-validation-for="@Model.Indtaegt.Indtaegt_ID" class="text-danger"></span>
            </div>


            <div class="form-group">
                <label asp-for="@Model.Indtaegt.Salg.Salg_ID" class="control-label"></label>
                <input asp-for="@Model.Indtaegt.Salg.Salg_ID" class="form-control" />
                <span asp-validation-for="@Model.Indtaegt.Salg.Salg_ID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
                <div class="center-container">
                    <a asp-page="/Indtaegter/GetIndtaegter">Tilbage til Indtægter</a>
                </div>
    </div>
</div>
</div>
