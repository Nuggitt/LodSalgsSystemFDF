@page
@model LodSalgsSystemFDF.Pages.Ledere.CreateLederModel
@{
    ViewData["Title"] = "CreateLeder";
}
<h1 style="margin-top:80px">
<h1>Opret ny leder</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="  ModelOnly  " class="text-danger">
            </div>

            <div class="form-group">
                <label asp-for="@Model.Leder.Leder_ID" class="control-label"></label>
                <input asp-for="@Model.Leder.Leder_ID" class="form-control" />
                <span asp-validation-for="@Model.Leder.Leder_ID" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="@Model.Leder.Navn" class="control-label"></label>
                <input asp-for="@Model.Leder.Navn" class="form-control" />
                <span asp-validation-for="@Model.Leder.Navn" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="@Model.Leder.Adresse" class="control-label"></label>
                <input asp-for="@Model.Leder.Adresse" class="form-control" />
                <span asp-validation-for="@Model.Leder.Adresse" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="@Model.Leder.Telefon" class="control-label"></label>
                <input asp-for="@Model.Leder.Telefon" class="form-control" />
                <span asp-validation-for="@Model.Leder.Telefon" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="@Model.Leder.Email" class="control-label"></label>
                <input asp-for="@Model.Leder.Email" class="form-control" />
                <span asp-validation-for="@Model.Leder.Email" class="text-danger"></span>
            </div>

           @* <div class="form-group">
                <label asp-for="@Model.Leder.ErLotteriBestyrer" class="control-label"></label>
                <input asp-for="@Model.Leder.ErLotteriBestyrer" class="form-control" />
                <span asp-validation-for="@Model.Leder.ErLotteriBestyrer" class="text-danger"></span>
            </div>*@

            <div class="form-group">
                <label asp-for="@Model.Leder.Børnegruppe_ID" class="control-label"></label>
                <input asp-for="@Model.Leder.Børnegruppe_ID" class="form-control" />
                <span asp-validation-for="@Model.Leder.Børnegruppe_ID" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>